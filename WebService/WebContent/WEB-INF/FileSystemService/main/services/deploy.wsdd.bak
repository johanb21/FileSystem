<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from FileSystemService WSDL service -->

  <service name="FileSystem" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://services.main"/>
      <parameter name="wsdlServiceElement" value="FileSystemService"/>
      <parameter name="schemaQualified" value="http://services.main,http://main"/>
      <parameter name="wsdlServicePort" value="FileSystem"/>
      <parameter name="className" value="main.services.FileSystemSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="FileSystem"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="execCommand" qname="operNS:execCommand" xmlns:operNS="http://services.main" returnQName="retNS:execCommandReturn" xmlns:retNS="http://services.main" returnType="rtns:BeanFileSystem" xmlns:rtns="http://main" soapAction="" >
        <parameter qname="pns:obj" xmlns:pns="http://services.main" type="tns:BeanFileSystem" xmlns:tns="http://main"/>
      </operation>
      <parameter name="allowedMethods" value="execCommand"/>

      <typeMapping
        xmlns:ns="http://main"
        qname="ns:BeanFileSystem"
        type="java:main.BeanFileSystem"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
  </service>
</deployment>
